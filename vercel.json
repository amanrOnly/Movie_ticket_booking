{
    "version": 2,
    "builds": [
        {
            "src": "movie_book_api/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {"maxLambdaSize": "15mb"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "movie_book_api/wsgi.py"
        }
    ]
}